<h1>New Blog</h1>

<%= form_for(@blog) do |f| %>
  

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
</p>

<p>
    <%= f.label :desc %><br>
    <%= f.text_area :desc %>
  </p>

  <p>
  
    <%= f.label :created_by %><br>
    <%= f.text_field :created_by %>
  </p>

  <p>
    <%= f.submit %>
</p>
<p>
<% end %>
</p>