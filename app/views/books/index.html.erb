

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Desc</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
    <td><%= book.title %></td>
        <td><%= book.location %></td>
        <td><%= book.created_by %></td>
        <td><%= book.category_id %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

